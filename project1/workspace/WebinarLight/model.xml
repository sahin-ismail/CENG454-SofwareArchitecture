					</and>
				</and>
			</and>
		</and>
	</struct>
	<constraints>
		<rule>
			<eq>
				<var>LiveStreaming</var>
				<var>Live</var>
			</eq>
		</rule>
		<rule>
			<disj>
				<var>Live</var>
				<var>PreRecorded</var>
			</disj>
		</rule>
	</constraints>
</featureModel>
